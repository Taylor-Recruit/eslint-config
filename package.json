{
  "name": "@taylor-recruit/eslint-config",
  "version": "0.0.2",
  "description": "ESLint configuration used by Taylor Recruit team",
  "homepage": "https://github.com/Taylor-Recruit/infra-eslint-config",
  "author": "Noushad <to.noushadpp@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Taylor-Recruit/infra-eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config"
  ],
  "main": "index.js",
  "scripts": {
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint": "yarn eslint .",
    "prettier": "prettier --ignore-unknown",
    "format:check": "yarn prettier --check .",
    "format:fix": "yarn prettier --write .",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "1.3.3",
    "confusing-browser-globals": "1.0.11",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-vitest": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/eslint-parser": "7.22.10",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@semantic-release/gitlab": "12.0.5",
    "@semantic-release/npm": "10.0.5",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-testing-library": "^",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "pinst": "3.0.0",
    "prettier": "3.0.2",
    "semantic-release": "21.0.9",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-react": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.5.0 | ^9.0.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^6.0.0",
    "prettier": "^3.0.0",
    "typescript": "^4 | ^5"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/preset-react": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jest-formatting": {
      "optional": true
    },
    "eslint-plugin-vitest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-prettier": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "release": {
    "branches": "main",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  },
  "lint-staged": {
    "*.js": "yarn lint --max-warnings=0 --fix",
    "!*.js": "yarn prettier --write"
  }
}
